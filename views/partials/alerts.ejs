<% if (alerts.error) { %>
    <% alerts.error.forEach(function(message) { %>
        <div style="background-color: red; color: white;">
        <%= message %>
        </div>
    <% }); %>
<% } else if (alert.success) { %>
    <% alert.success.forEach(function(message) { %>
        <div style="background: green; color: white;">
            <%= message %>
        </div>
    <% }); %>
<% } %>

    
